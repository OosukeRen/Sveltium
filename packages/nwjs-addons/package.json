{
  "name": "nwjs-addons",
  "version": "0.1.0",
  "description": "Native C++ addons for NW.js applications (Windows XP compatible)",
  "main": "./index.js",
  "exports": {
    ".": "./index.js",
    "./clipboard": "./clipboard/index.js",
    "./folder-dialog": "./folder-dialog/index.js",
    "./ipc": "./ipc/index.js",
    "./nw-sqlite3": "./nw-sqlite3/index.js",
    "./tinycc": "./tinycc/index.js",
    "./call-dll": "./call-dll/index.js",
    "./csv-parser": "./csv-parser/index.js",
    "./rss-parser": "./rss-parser/index.js",
    "./sdl2-input": "./sdl2-input/index.js"
  },
  "files": [
    "index.js",
    "install.js",
    "CMakeLists.txt",
    "src",
    "clipboard",
    "folder-dialog",
    "ipc",
    "nw-sqlite3",
    "tinycc",
    "call-dll",
    "csv-parser",
    "rss-parser",
    "sdl2-input",
    "build"
  ],
  "scripts": {
    "install": "node install.js",
    "build": "cmake-js compile --runtime nw --runtime-version 0.12.3 --arch ia32 -G \"Visual Studio 14 2015\" -t v140_xp",
    "build:xp": "cmake-js compile --runtime nw --runtime-version 0.12.3 --arch ia32 -G \"Visual Studio 14 2015\" -t v140_xp",
    "build:modern": "cmake-js compile --runtime nw --runtime-version 0.89.0 --arch x64",
    "build:debug": "cmake-js build --runtime nw --runtime-version 0.12.3 --arch ia32 -G \"Visual Studio 14 2015\" -t v140_xp -D",
    "rebuild": "cmake-js rebuild --runtime nw --runtime-version 0.12.3 --arch ia32 -G \"Visual Studio 14 2015\" -t v140_xp",
    "rebuild:modern": "cmake-js rebuild --runtime nw --runtime-version 0.89.0 --arch x64",
    "clean": "cmake-js clean"
  },
  "keywords": [
    "nwjs",
    "native",
    "addon",
    "clipboard",
    "folder-dialog",
    "ipc",
    "sqlite3",
    "tinycc",
    "ffi",
    "dll",
    "csv",
    "csv-parser",
    "rss",
    "rss-parser",
    "atom",
    "feed",
    "sdl2",
    "joystick",
    "gamepad",
    "controller",
    "input",
    "rumble",
    "windows-xp"
  ],
  "author": {
    "name": "Oosuke Ren",
    "email": "oosukeren@gmail.com",
    "url": "https://github.com/OosukeRen"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/OosukeRen/Sveltium",
    "directory": "packages/nwjs-addons"
  },
  "devDependencies": {
    "cmake-js": "4.0.0",
    "nan": "2.5.1"
  },
  "cmake-js": {
    "runtime": "nw",
    "runtimeVersion": "0.12.3",
    "arch": "ia32"
  }
}
